    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "4.12",
    "multer": "^1.4.5-lts.1",
    "serialize-error": "^7.0.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^14.0.14",
    "@types/shortid": "^0.0.29",
