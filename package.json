{
  "name": "talktalk-server",
  "version": "1.0.0",
  "description": "backend done with expressjs for TalkTalk",
  "main": "dist/index.js",
  "scripts": {
    "lint": "npx eslint './**/*.ts'",
    "build": "npx tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register index.ts",
    "test": "cross-env NODE_ENV=development npx mocha --require ts-node/register --require tsconfig-paths/register 'test/**/*.test.ts' --watch --watch-files './**/*.ts'"
  },
  "keywords": [
    "node",
    "express",
    "backend"
  ],
  "author": "fjplaurr",
  "license": "MIT",
  "dependencies": {
    "argon2": "^0.30.2",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "4.12",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "serialize-error": "^7.0.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^14.0.14",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.3.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.4",
    "typescript-eslint": "^8.12.2"
  }
}
